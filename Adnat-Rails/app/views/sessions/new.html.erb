<h2>Login</h2>

<%= form_tag sessions_path do %>
  <%= label_tag :Email %><br />
  <%= text_field_tag :email, params[:email] %><br/><br/>
  <%= label_tag :Password %><br />
  <%= password_field_tag :password %><br/>
  <p class="btn"><%= submit_tag "Login" %></p>
  <%= link_to "Signup", signup_path %><br/>
<% end %>